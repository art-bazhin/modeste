{
  "name": "modeste",
  "description": "Library for rendering user inerfaces using tagged template literals.",
  "homepage": "https://github.com/art-bazhin/modeste",
  "repository": "git@github.com/art-bazhin/modeste.git",
  "bugs": "https://github.com/art-bazhin/modeste/issues",
  "version": "0.2.8",
  "main": "dist/modeste.js",
  "types": "dist/modeste.d.ts",
  "license": "MIT",
  "devDependencies": {
    "cz-conventional-changelog": "^3.2.0",
    "prettier": "^2.0.5",
    "rollup": "^2.16.1",
    "rollup-plugin-ts": "^1.2.24",
    "rollup-plugin-uglify": "^6.0.4",
    "standard-version": "^8.0.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "files": [
    "dist"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
