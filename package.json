{
  "name": "modeste",
  "description": "Library for rendering user inerfaces using tagged template literals.",
  "homepage": "https://github.com/art-bazhin/modeste",
  "repository": "git@github.com/art-bazhin/modeste.git",
  "bugs": "https://github.com/art-bazhin/modeste/issues",
  "version": "0.3.5",
  "main": "dist/modeste.js",
  "browser": "dist/modeste.min.js",
  "unpkg": "dist/modeste.min.js",
  "types": "dist/modeste.d.ts",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.5",
    "babel-jest": "^26.1.0",
    "cz-conventional-changelog": "^3.2.0",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "rollup": "^2.22.1",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-ts": "^1.2.24",
    "standard-version": "^8.0.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:dev": "jest --watch",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "files": [
    "dist"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
