{
  "name": "modeste",
  "description": "Library for rendering user inerfaces using tagged template literals.",
  "homepage": "https://github.com/art-bazhin/modeste",
  "repository": "git@github.com/art-bazhin/modeste.git",
  "bugs": "https://github.com/art-bazhin/modeste/issues",
  "version": "0.0.4",
  "main": "dist/modeste.js",
  "browser": "dist/modeste.min.js",
  "types": "dist/modeste.d.ts",
  "devDependencies": {
    "cz-conventional-changelog": "^3.0.2",
    "prettier": "^1.16.4",
    "rollup": "^1.23.1",
    "rollup-plugin-ts": "^0.2.0",
    "rollup-plugin-uglify": "^6.0.3",
    "standard-version": "^7.0.0",
    "tslib": "^1.10.0",
    "typescript": "^3.6.3"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "files": [
    "dist"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
